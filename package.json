{
  "name": "mobx-async-state",
  "version": "1.0.1",
  "description": "State management of mobx async function.",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "npx dumi dev",
    "doc": "npx dumi build",
    "build": "rm -rf lib && babel src --out-dir lib --ignore 'src/**/demo/**/*','src/**/*.md','src/.umi'",
    "build:doc": "npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caijf/mobx-async-state.git"
  },
  "keywords": [
    "mobx",
    "async",
    "state",
    "react"
  ],
  "author": "caijf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caijf/mobx-async-state/issues"
  },
  "homepage": "https://github.com/caijf/mobx-async-state#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^1.13.0",
    "antd": "^4.1.0",
    "babel-plugin-import": "^1.13.0",
    "dayjs": "^1.8.36",
    "dumi": "^1.0.31",
    "eslint-plugin-react": "^7.21.2",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "mockjs": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "mobx": ">=4.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": [
            "> 1%",
            "last 4 versions",
            "Firefox ESR",
            "not ie < 9"
          ]
        }
      ]
    ]
  }
}
